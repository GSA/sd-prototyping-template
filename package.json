{
  "name": "sd-prototyping-template",
  "description": "A generic prototyping template to quickly build a site using USWDS",
  "scripts": {
    "start": "concurrently npm:uswds:watch npm:serve",
    "serve": "eleventy --serve",
    "uswds:watch": "gulp watch",
    "uswds:compile": "gulp compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GSA/sd-prototyping-template.git"
  },
  "license": "PUBLIC DOMAIN",
  "bugs": {
    "url": "https://github.com/GSA/sd-prototyping-template/issues"
  },
  "homepage": "https://github.com/GSA/sd-prototyping-template#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@uswds/compile": "^1.0.0-beta.3",
    "@uswds/uswds": "^3.0.2",
    "concurrently": "^7.2.2",
    "gulp": "^4.0.2"
  }
}
